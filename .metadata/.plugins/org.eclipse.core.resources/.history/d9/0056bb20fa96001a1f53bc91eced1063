#include"rectangle.h" /*include shape class interfaces*/
#include"math.h"
void rectangle_constructor(rectangle *me, int16_t x0, int16_t y0, uint16_t w0, uint16_t h0)
{
	//me->rect.x=x0;
	shape_constructor(&me->rect,x0,y0);
	me->hight=h0;
	me->width=w0;
}


